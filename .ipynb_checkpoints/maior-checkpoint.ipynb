    "def maior(lista, tam, i=0):\n",
    "    if tam==0:\n",
    "        return lista[i]\n",
    "    else:\n",
    "        if lista[tam-1] > lista[i]:\n",
    "            return maior(lista, tam-1, tam-1)\n",
    "        else:\n",
    "            return maior(lista, tam-1, i)\n",
    "    "
